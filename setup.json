{
    "name": "aiida-lsmo",
    "author": "Aliaksandr Yakutovich, Daniele Ongari",
    "author_email": "aliaksandr.yakutovich@epfl.ch",
    "description": "AiiDA workflows for the LSMO laboratory at EPFL",
    "url": "https://github.com/yakutovicha/aiida-lsmo-workflows",
    "license": "MIT License",
    "classifiers": [
        "Programming Language :: Python :: 3.6",
        "Programming Language :: Python :: 3.7"
    ],
    "version": "1.0.0a1",
    "setup_requires": ["reentry"],
    "reentry_register": true,
    "install_requires": [
        "aiida_core >= 1.0.0b6"
    ],
    "entry_points": {
        "aiida.workflows": [
            "aiida_lsmo.isotherm = aiida_lsmo.workchains:IsothermWorkChain"
        ]
    },
    "data_files": [
        [".", ["setup.json"]]
    ],
    "extras_require": {
        "testing": [
            "pytest==4.4.1"
        ],
        "pre-commit": [
                "pre-commit==1.17.0",
                "yapf==0.28.0",
                "prospector==1.1.7",
                "pylint==2.3.1"
        ]
    }
}
